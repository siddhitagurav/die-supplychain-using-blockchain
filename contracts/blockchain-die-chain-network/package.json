{
  "engines": {
    "composer": "^0.20.7"
  },
  "name": "blockchain-die-chain-network",
  "version": "0.0.1",
  "description": "supply chain",
  "scripts": {
    "prepublish": "mkdirp ./dist && composer archive create --sourceType dir --sourceName . -a ./dist/blockchain-die-chain-network.bna",
    "pretest": "npm run lint",
    "lint": "eslint .",
   /* "postlint":"npm run licchk",
    "licchk": "license-check",
    "postlicchk":"npm run doc",
    "doc":"jsdoc --pedantic --recurse -c jsdoc.json",*/
    "test": "nyc mocha -t 0 test/*.js && cucumber-js"
    "deploy": "./scripts/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/siddhitagurav/die-supplychain-using-blockchain.git"
  },
  "keywords": [
    "composer",
    "composer-network"
  ],
  "author": "ysh",
  "email": "ysh@gmail.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "composer-admin": "^0.20.7",
    "composer-cli": "^0.20.7",
    "composer-client": "^0.20.7",
    "composer-common": "^0.20.7",
    "composer-connector-embedded": "^0.20.7",
    "composer-cucumber-steps": "^0.20.7",
    "chai": "latest",
    "chai-as-promised": "latest",
    "cucumber": "^2.2.0",
    "eslint": "latest",
    "nyc": "latest",
    "mkdirp": "latest",
    "mocha": "latest"
  },
    "license-check-config": {
    "src": [
      "**/*.js",
      "!./coverage/**/*",
      "!./node_modules/**/*",
      "!./out/**/*",
      "!./scripts/**/*"
    ],
    "path": "header.txt",
    "blocking": true,
    "logInfo": false,
    "logError": true
  }
}
}
